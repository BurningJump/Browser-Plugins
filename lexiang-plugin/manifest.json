{
  "name": "lexiang-plugin",
  "version": "0.9.0",
  "manifest_version": 2,
  "description": "乐享课程自动学习",
  "browser_action": {
    "default_icon": "favicon.png",
    "default_title": "lexiang"
  },
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://lexiangla.com/classes/*"],
      "js": ["main.js"],
      "run_at": "document_idle"
    }
  ]
}